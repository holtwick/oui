{
  "name": "oui-kit",
  "type": "module",
  "version": "0.11.0",
  "author": {
    "name": "Dirk Holtwick",
    "email": "dirk.holtwick@gmail.com",
    "url": "https://holtwick.de"
  },
  "license": "MIT",
  "funding": {
    "type": "GitHub Sponsors ❤",
    "url": "https://github.com/sponsors/holtwick"
  },
  "homepage": "https://github.com/holtwick/oui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holtwick/oui.git"
  },
  "bugs": {
    "url": "https://github.com/holtwick/oui/issues"
  },
  "keywords": [
    "css",
    "framework",
    "stylus",
    "oui",
    "oui-kit",
    "vue",
    "ui",
    "typescript",
    "headless"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js"
    },
    "./css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./stylus": {
      "import": "./stylus/index.styl",
      "require": "./stylus/index.styl"
    },
    "./theme": {
      "import": "./stylus/theme.styl",
      "require": "./stylus/theme.styl"
    }
  },
  "module": "./dist/lib.js",
  "types": "./dist/lib.d.ts",
  "files": [
    "dist",
    "stylus"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "vite build",
    "check": "vue-tsc --noEmit",
    "dev": "vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "preview": "vite preview",
    "start": "nr story:dev",
    "story:build": "histoire build",
    "story:dev": "histoire dev",
    "story:preview": "histoire preview",
    "upload": "nr story:build && rsync -rvz .histoire/dist/* cy:public_html/oui.holtwick.de",
    "test": "vitest"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.6",
    "@vueuse/core": "^10.9.0",
    "vue": "^3.4.21",
    "zeed": "^0.17.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.0",
    "@antfu/ni": "^0.21.12",
    "@histoire/plugin-vue": "^0.17.12",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.25",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "fast-glob": "^3.3.2",
    "histoire": "^0.17.9",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "only-allow": "^1.2.1",
    "sort-package-json": "^2.8.0",
    "stylus": "^0.63.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.6"
  }
}
